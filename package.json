{
  "name": "unrouting",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.14.0",
  "description": "",
  "license": "MIT",
  "repository": "unjs/unrouting",
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "lint": "eslint .",
