  "dependencies": {
    "escape-string-regexp": "^5.0.0",
    "ufo": "^1.3.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "latest",
    "@types/node": "22.17.0",
    "@vitest/coverage-v8": "latest",
    "bumpp": "10.2.2",
    "eslint": "latest",
    "lint-staged": "latest",
    "rou3": "^0.7.3",
    "simple-git-hooks": "latest",
    "typescript": "latest",
    "unbuild": "latest",
    "vite": "latest",
    "vitest": "latest",
    "vue-router": "4.5.1"
  },
  "resolutions": {
    "unrouting": "link:."
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,mjs,cjs,json,.*rc}": [
